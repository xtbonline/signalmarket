<div class="settings mtb15">
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-12 col-lg-3">
        <%- include("./partials/sidenav") %>
      </div>
      <div class="col-md-12 col-lg-9">
        <div class="tab-content" id="v-pills-tabContent">

          <div class="tab-pane fade active show" id="withdraw" role="tabpanel" aria-labelledby="wallet-tab">
            <div class="wallet">
              <div class="row">
                <div class="col-md-12 col-lg-8 mr-2">
                  <div class="tab-content">
                    <div class="tab-pane fade show active" id="coinBTC" role="tabpanel">
                      <% if (error_msg !="" ) { %>
                      <div class="alert alert-danger d-flex align-items-center" role="alert">
                        <svg class="bi flex-shrink-0 me-2" width="24" height="24" role="img" aria-label="Danger:">
                          <use xlink:href="#exclamation-triangle-fill" />
                        </svg>
                        <div>
                          <strong>Error: </strong>
                          <%=error_msg%>
                        </div>
                      </div>
                      <% } %>
                      <% if (success_msg !="" ) { %>
                      <div class="alert alert-success d-flex align-items-center" role="alert">
                        <svg class="bi flex-shrink-0 me-2" width="24" height="24" role="img" aria-label="Success:">
                          <use xlink:href="#check-circle-fill" />
                        </svg>
                        <div>
                          <strong>Success: </strong>
                          <%= success_msg %>
                        </div>
                      </div>
                      <% } %>
                      <!-- <div class="vh-100 d-flex justify-content-center"> -->

                      <div class="form-access mx-auto">
                        <form action="/dashboard/withdraw" method="post">
                          <span>Withdraw asset</span>
                          <p>The minimum withdrawal limit is 15,000 and above.</p>
                          <div class="form-group">
                            <!-- <input type="text" name="address" class="form-control" placeholder="Asset address from"> -->
                            <select class="form-select" aria-label="Default select example" name="coin_name">
                              <option selected value="default">Choose coin</option>
                              <option value="Bitcoin">Bitcoin</option>
                              <option value="Ethereum">Ethereum</option>
                              <option value="Usdt erc20">Usdt erc20</option>
                              <option value="Usdt trc20">Usdt trc20</option>
                            </select>
                          </div>
                          <div class="form-group">
                            <input type="text" name="address" class="form-control" placeholder="Wallet Address">
                          </div>
                          <div class="form-group">
                            <input type="text" name="amount" class="form-control" placeholder="Amount">
                          </div>
                          <button type="submit" class="btn btn-primary">Withdraw</button>
                        </form>
                      </div>
                      <!-- </div> -->

                    </div>

                  </div>
                </div>
              </div>
            </div>
          </div>


        </div>
      </div>
    </div>
  </div>
</div>
